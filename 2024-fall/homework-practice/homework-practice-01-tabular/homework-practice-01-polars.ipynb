    "def distribute_for_nth_course(dff, n, courses_and_places, course1 = 'course1', course2 = 'course2'):\n",
    "        \n",
    "    for course, N in courses_and_places.items(): # (–∏—Ç–µ—Ä–∞—Ü–∏—è –ø–æ –∫—É—Ä—Å–∞–º - –º–æ–∂–Ω–æ)   \n",
    "\n",
    "        is_first_course = (\n",
    "            (pl.col('num') == 1)\n",
    "            .and_(pl.col(priorities[n]) == course)\n",
    "        )\n",
    "            \n",
    "        is_second_course = (\n",
    "            (pl.col('num') == 2)\n",
    "            .and_(\n",
    "                (pl.col(priorities[n]) == course)\n",
    "                .or_(pl.col(priorities[n + 1]) == course)\n",
    "            )\n",
    "        )\n",
    "        \n",
    "        is_in_this_course = (\n",
    "            (pl.col(course1) == course)\n",
    "            .or_(pl.col(course2) == course)\n",
    "        )\n",
    "        \n",
    "        nN = (\n",
    "            dff\n",
    "            .filter(is_in_this_course)\n",
    "            .shape[0]\n",
    "        )\n",
    "        \n",
    "        cutoff_df = ( # —Ç–æ–ø N - nN —Å —ç—Ç–∏–º–∏ –ø–µ—Ä—Ü–µ–Ω—Ç–∏–ª—è–º–∏ –ø—Ä–æ–π–¥—É—Ç –Ω–∞ –∫—É—Ä—Å –≤ —ç—Ç–æ–π –≤–æ–ª–Ω–µ\n",
    "            dff\n",
    "            .filter(\n",
    "                (\n",
    "                    is_first_course\n",
    "                    .and_(pl.col(course1).is_null())\n",
    "                )\n",
    "                .or_(\n",
    "                    is_second_course\n",
    "                    .and_(\n",
    "                        (\n",
    "                            (pl.col(course1).is_null())\n",
    "                            .and_(pl.col(course2).is_null())\n",
    "                        )\n",
    "                        .or_(\n",
    "                            (pl.col(course1).is_null())\n",
    "                            .and_(pl.col(course2).is_not_null())\n",
    "                            .and_(pl.col(course2) != course)\n",
    "                        )\n",
    "                        .or_(\n",
    "                            (pl.col(course2).is_null())\n",
    "                            .and_(pl.col(course1).is_not_null())\n",
    "                            .and_(pl.col(course1) != course)\n",
    "                        )\n",
    "                    )\n",
    "                )\n",
    "            )\n",
    "            .select('percentile')\n",
    "            .slice(0, N - nN)\n",
    "        )\n",
    "                \n",
    "                        \n",
    "        if cutoff_df.shape[0] == 0:\n",
    "            continue\n",
    "                        \n",
    "        last_val = cutoff_df.tail(1).item()\n",
    "          \n",
    "        dff = (\n",
    "            dff\n",
    "            .with_columns( \n",
    "                pl.when( # —Ä–∞—Å–ø—Ä–µ–¥–µ–ª—è–µ–º –Ω–∞ 1 –ö–ü–í –µ—Å–ª–∏ –Ω–µ –±—ã–ª —Ä–∞—Å–ø—Ä–µ–¥–µ–ª–µ–Ω\n",
    "                    (\n",
    "                        is_first_course | is_second_course\n",
    "                    )\n",
    "                    .and_(pl.col('percentile') <= last_val)\n",
    "                    .and_(pl.col(course1).is_null())\n",
    "                )\n",
    "                .then(pl.lit(course))\n",
    "                .otherwise(course1)\n",
    "                .alias(course1)\n",
    "            )\n",
    "\n",
    "            .with_columns( \n",
    "                pl.when( # —Ä–∞—Å–ø—Ä–µ–¥–µ–ª—è–µ–º –Ω–∞ 2 –ö–ü–í –µ—Å–ª–∏ –Ω–µ –±—ã–ª —Ä–∞—Å–ø—Ä–µ–¥–µ–ª–µ–Ω\n",
    "                    is_second_course\n",
    "                    .and_(pl.col('percentile') <= last_val)\n",
    "                    .and_(pl.col(course2).is_null())\n",
    "                    .and_(pl.col(course1) != course)\n",
    "                )\n",
    "                .then(pl.lit(course))\n",
    "                .otherwise(course2)\n",
    "                .alias(course2),\n",
    "            )\n",
    "        )\n",
    "\n",
    "    \n",
    "    return dff"
   ]
