   "source": [
    "import plotly.express as px\n",
    "import plotly.io as pio\n",
    "\n",
    "plt_df = (\n",
    "    df\n",
    "    .with_columns(\n",
    "        pl.col(\"timestamp\")\n",
    "        .str.strptime(pl.Datetime)\n",
    "        .dt.strftime(\"%Y-%m-%d\")\n",
    "    )\n",
    "    .select('timestamp', 'rating')\n",
    "    .group_by('timestamp')\n",
    "    .mean()\n",
    "    .collect()    \n",
    ")\n",
    "\n",
    "fig = px.bar(x=plt_df['timestamp'], y=plt_df['rating'], title='График средних рейтингов по дням получения ответов')\n",
    "\n",
    "fig.update_xaxes(title='timestamp')\n",
    "fig.update_yaxes(title='rating')\n",
    "\n",
    "\n",
    "fig.show(renderer='png')"
   ]
